{
    "name": "lastzero/test-tools",
    "type": "library",
    "description": "Test Tools for PHPUnit",
    "keywords": ["phpunit","testing","database","db","dependency injection","doctrine","dbal","mock","fixture"],
    "homepage": "https://github.com/lastzero/test-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Mayer",
            "email": "michael@lastzero.net",
            "homepage": "https://lastzero.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/dependency-injection": ">=2.8",
        "symfony/yaml": ">=2.8",
        "symfony/config": ">=2.8",
        "mockery/mockery": "~0.9"
    },
    "require-dev": {
        "symfony/console": ">=2.8",
        "symfony/framework-bundle": ">=2.8",
        "doctrine/dbal": "~2.5",
        "guzzle/guzzle": "~3.9.2",
        "kriswallsmith/buzz": "~0.10",
        "basho/riak": "~2.0",
        "swiftmailer/swiftmailer": "5.*",
        "phpunit/phpunit": "@stable"
    },
    "autoload": {
        "psr-4": {
            "TestTools\\": "src/TestTools"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
